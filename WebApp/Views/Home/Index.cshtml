@{
    ViewData["Title"] = "Home page";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <h1>Page content</h1>
    <ol type="A">
        @for (int i = 1; i < 5; i++)
        {
            <li>Tag #@i</li>
        }
    </ol>
    <div>@ViewData["Text"]</div>
    <div>@ViewData["Value"]</div>
</div>
